[flake8]
max-line-length = 88
extend-ignore = E203, E501, W503
exclude =
    .git,
    __pycache__,
    .mypy_cache,
    .pytest_cache,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    Versao_Linux,
    Versao_Windows,
    .old

per-file-ignores =
    tests/*:S101
    __init__.py:F401

max-complexity = 10
